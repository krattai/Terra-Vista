#VRML V2.0 utf8
#
# Innards

# land
Shape {
 appearance Appearance {
  material Material { diffuseColor 1.0 1.0 1.0 ambientIntensity 0.9 }
 }
 geometry IndexedFaceSet {
  coord Coordinate { point [ -100 0 -100, 100 0 -100, 100 0 100, -100 0 100. ] }
  coordIndex [ 2, 1, 0, -1, 3, 2, 0, -1, ]
 }
}

# Fluxport
Transform {
 translation 60 50 -100
 children [
  Inline { url "http://home.t-online.de/home/kiwano2/fluxport.wrl" bboxSize 6 7 6 bboxCenter 0 3.5 0 }
 ]
}

# To next segment
Transform {
 translation -90 1.5 0
 children [
  Anchor {
   children [
    Shape {
     appearance Appearance {
      material Material { diffuseColor 0.4 0.4 0.75 }
      texture ImageTexture {
       url "nextsegm.gif"
      }
     }
     geometry Box { size 2 2 0.1 }
    }
    Transform {
     translation -1.0 1.0 0.0
     children [
      Shape {
       appearance Appearance {
        material Material { diffuseColor 0.9 0.9 0.0 ambientIntensity 0.4 }
       }
       geometry Text {
        string [ "Next Segment 03" ]
       }
      }
     ]
    }
   ]
   description "Next Segment 03"
   url "bs03.wrl"
  }
 ]
}

